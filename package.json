{
  "name": "mdify-node",
  "version": "1.0.2",
  "description": "Convert DOCX files to markdown.",
  "keywords": [
    "nodejs",
    "docx-to-markdown",
    "docx",
    "markdown",
    "mdify-node"
  ],
  "author": {
    "name": "Stefan StÃ¶lzle",
    "email": "stefan@github.com",
    "url": "stefan.stoelzle.me"
  },
  "license": "MIT",
  "homepage": "https://github.com/stoe/mdify-node#readme",
  "repository": "stoe/mdify-node",
  "bugs": {
    "url": "https://github.com/stoe/mdify-node/issues"
  },
  "engines": {
    "node": ">=6"
  },
  "bin": {
